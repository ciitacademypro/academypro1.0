@model Firm

@{
    ViewData["Title"] = "Firm Details";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}


<div class="container">
    <div class="row">
        <div class="col-lg-12">
            <a class="btn btn-default btn-success" style="float:right; position:relative;" asp-action="edit" asp-route-id="@Model?.FirmId"> Edit</a>
            <a class="btn btn-default btn-success" style="float:right; position:relative;" asp-action="Index"> Back to List</a>
            <h2 class="st_title"><i class="uil uil-analysis"></i> Firm Details</h2>
            @await Html.PartialAsync("~/Areas/Admin/Views/Partials/_AlertPartial.cshtml")
        </div>
    </div>

    <div class="row">
        <div class="col-lg-12">
                <div class="table-responsive mt-30">
                    <dl class="row table">

                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.FirmShortName)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.FirmShortName)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.FirmName)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.FirmName)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.FirmLogo)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.FirmLogo)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.FirmEmail)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.FirmEmail)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.FirmContact)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.FirmContact)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.FirmWeb)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.FirmWeb)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.FirmDescription)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.FirmDescription)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.Status)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.Status)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.DeletedAt)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.DeletedAt)
                    </dd>
                </dl>
                </div>
        </div>
    </div>
</div>


